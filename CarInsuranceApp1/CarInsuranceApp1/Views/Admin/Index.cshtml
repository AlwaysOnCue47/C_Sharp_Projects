@model IEnumerable<CarInsuranceApp1.Models.Table1Insurees>

@{
    ViewBag.Title = "Index";
}

<h2>List of Insurees</h2>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Quote</th> <!-- Display the calculated quote -->
    </tr>

    @foreach (var insuree in Model)
    {
        <tr>
            <td>@insuree.FirstName</td>
            <td>@insuree.LastName</td>
            <td>@insuree.EmailAddress</td>
            <td>@insuree.Quote.ToString("C")</td> <!-- Format as currency -->
        </tr>
    }
</table>